<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="ru.magnat.android.service.openvpn.client"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="19" />
    
    <uses-permission android:name="magnat.android.permission.OPENVPN_SERVICE"/>
    <uses-permission android:name="de.blinkt.openvpn.REMOTE_API"/>
    
    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity android:name="ru.magnat.android.service.openvpn.client.MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>